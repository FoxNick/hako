<Project>
    <PropertyGroup>
        <DevBuild Condition="'$(DevBuild)'==''">true</DevBuild>
        <HakoVersion Condition="'$(HakoVersion)'==''">1.0.0</HakoVersion>
        <HakoDotnetVersion Condition="'$(HakoDotnetVersion)'==''"></HakoDotnetVersion>
        <HakoPackageVersion Condition="'$(DevBuild)'=='true'">$(HakoVersion)$(HakoDotnetVersion)-dev</HakoPackageVersion>
        <HakoPackageVersion Condition="'$(HakoPackageVersion)'==''">$(HakoVersion)$(HakoDotnetVersion)</HakoPackageVersion>
    </PropertyGroup>

    <PropertyGroup Condition="'$(Configuration)' == 'Debug'">
        <EnableSourceLink>false</EnableSourceLink>
        <EnableSourceControlManagerQueries>false</EnableSourceControlManagerQueries>
    </PropertyGroup>
</Project>